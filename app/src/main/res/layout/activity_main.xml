        android:textSize="48dp"
        app:layout_constraintEnd_toStartOf="@+id/auto_upper_plus"
        app:layout_constraintStart_toEndOf="@+id/hubauto2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
